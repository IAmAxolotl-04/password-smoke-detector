{
  "protocolName": "Credential Shield Protocol (CSP)",
  "version": "csp-1.0-draft",
  "referenceImplementation": "CSPPrefixHasher",
  "algorithms": [
    "sha256",
    "sha384",
    "sha512"
  ],
  "errorCodes": {
    "INVALID_ALGORITHM": "CSP_ERR_001",
